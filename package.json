{
    "dependencies": {
        "location": "0.0.1",
        "ol": "^5.3.3",
        "sidebar-v2": "^0.4.0",
        "style": "0.0.3"
    },
    "devDependencies": {
        "concurrently": "^4.1.0",
        "eslint": "^5.16.0",
        "eslint-config-standard": "^12.0.0",
        "eslint-plugin-import": "^2.17.3",
        "eslint-plugin-node": "^9.1.0",
        "eslint-plugin-promise": "^4.1.1",
        "eslint-plugin-standard": "^4.0.0",
        "parcel-bundler": "^1.12.3",
        "sass": "^1.21.0"
    },
    "scripts": {
        "test": "eslint resources/javascript/",
        "fix": "eslint resources/javascript/ --fix",
        "watch": "concurrently --raw \"npm:watch:*\"",
        "watch:map": "parcel watch --out-dir=public/ --out-file=map.js resources/javascript/map.js --no-cache --no-hmr",
        "watch:table": "parcel watch --out-dir=public/ --out-file=table.js resources/javascript/table.js --no-cache --no-hmr",
        "build": "concurrently --raw \"npm:build:*\"",
        "build:map": "parcel build --out-dir=public/ --out-file=map.js --no-source-maps resources/javascript/map.js",
        "build:table": "parcel build --out-dir=public/ --out-file=table.js --no-source-maps resources/javascript/table.js"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
