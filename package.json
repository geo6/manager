{
    "dependencies": {
        "@babel/runtime": "^7.12.5",
        "ol": "^5.3.3",
        "sidebar-v2": "^0.4.0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/plugin-transform-runtime": "^7.12.10",
        "concurrently": "^5.3.0",
        "eslint": "^7.16.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.1.0",
        "parcel-bundler": "^1.12.4",
        "sass": "^1.30.0"
    },
    "scripts": {
        "postinstall": "npm run build",
        "test": "eslint resources/javascript/",
        "fix": "eslint resources/javascript/ --fix",
        "watch": "concurrently --raw \"npm:watch:*\"",
        "watch:map": "parcel watch --out-dir=public/ --out-file=map.js --global map resources/javascript/map.js --no-cache --no-hmr",
        "watch:table": "parcel watch --out-dir=public/ --out-file=table.js --global table resources/javascript/table.js --no-cache --no-hmr",
        "build": "concurrently --raw \"npm:build:*\"",
        "build:map": "parcel build --out-dir=public/ --out-file=map.js --global map resources/javascript/map.js --no-cache --no-source-maps",
        "build:table": "parcel build --out-dir=public/ --out-file=table.js --global table resources/javascript/table.js --no-cache --no-source-maps"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
